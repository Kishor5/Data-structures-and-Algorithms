# second largest element in array
nums = [55, 32, 97, 55, 45, 32, 54, 21]

largest = float("-inf")
second_largest = float("-inf")
n = len(nums)

for i in range(0,n):
    if nums[i] > largest:
        second_largest = largest
        largest = nums[i]
    elif nums[i] > second_largest and nums[i]!=largest:
        second_largest=nums[i]
print(second_largest)


