def Mergesort(a, b):
    i = 0
    j = 0
    result = []
    while i < len(a) and j < len(b):
        if a[i] <= b[j]:
            if len(result) == 0 or result[-1] !=a[i]:
                result.append(a[i])
                i +=1
        else:
            if len(result)==0 or result[-1] !=b[j]:
                result.append(b[j])
                j +=1
    while i < len(a):
        if len(result) == 0 or result[-1] != a[i]:
            result.append(a[i])
            i += 1
    while j < len(b):
        if len(result) == 0 or result[-1] != b[j]:
            result.append(b[j])
            j += 1
    return result
a = [1,3,5,6]
b = [1,2,4,4,7]
print(Mergesort(a,b))

